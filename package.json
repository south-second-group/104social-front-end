{
  "name": "nuxt3-template",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.12.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:view": "pnpm dlx eslint-flat-config-viewer",
    "add layout": "npx nuxi add layout default",
    "add page": "npx nuxi add page member/CommentItem",
    "add component": "npx nuxi add component utils/PaginationComp",
    "add composable": "npx nuxi add composable demo",
    "add api": "npx nuxi add api auth/demo.get",
    "lint:watch": "nodemon --exec 'npm run lint:fix' --watch . --ext .vue",
    "prepare": "husky"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@nuxt/devtools": "latest",
    "@nuxtjs/color-mode": "^3.3.3",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vueuse/nuxt": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "nuxt": "^3.11.1",
    "nuxt-easy-lightbox": "^1.0.2",
    "nuxt-simple-sitemap": "^4.4.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "sass": "^1.77.2",
    "tailwindcss": "^3.4.1",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.7"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-basic-styles": "^42.0.0",
    "@ckeditor/ckeditor5-build-classic": "^42.0.0",
    "@ckeditor/ckeditor5-editor-classic": "^42.0.0",
    "@ckeditor/ckeditor5-essentials": "^42.0.0",
    "@ckeditor/ckeditor5-font": "^42.0.0",
    "@ckeditor/ckeditor5-link": "^42.0.0",
    "@ckeditor/ckeditor5-paragraph": "^42.0.0",
    "@ckeditor/ckeditor5-theme-lark": "^42.0.0",
    "@ckeditor/ckeditor5-vue": "^6.0.0",
    "@ckeditor/vite-plugin-ckeditor5": "^0.1.3",
    "@google/generative-ai": "^0.11.4",
    "@iconify-json/heroicons": "^1.1.20",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@nuxt/image": "^1.7.0",
    "@nuxt/ui": "^2.15.2",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@vee-validate/zod": "^4.13.1",
    "@vueuse/core": "^10.10.0",
    "clsx": "^2.1.1",
    "isomorphic-dompurify": "^2.13.0",
    "nodemon": "^3.1.0",
    "nuxt-icon": "^0.6.10",
    "socket.io-client": "^4.7.5",
    "zod": "^3.23.8"
  }
}
